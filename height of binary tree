static int height(Node root) {
      	// Write your code here.
       if(root==null)
           return -1;
        else{
            int ld=height(root.left);
            int rd=height(root.right);
            if(ld>rd)
                return(ld+1);
            else
                return(rd+1);
        }
       // return 0;
    }
